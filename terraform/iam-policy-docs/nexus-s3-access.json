{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "NexusS3BucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3:::mpm-jenkins-cicd-pipeline-nexus-storage"
    },
    {
      "Sid": "NexusS3ObjectAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListMultipartUploadParts",
        "s3:AbortMultipartUpload"
      ],
      "Resource": "arn:aws:s3:::mpm-jenkins-cicd-pipeline-nexus-storage/*"
    }
  ]
}
